# DoggoHub logrotate settings
# based on: http://stackoverflow.com/a/4883967

/home/git/doggohub/log/*.log {
    daily
    missingok
    rotate 90
    compress
    notifempty
    copytruncate
}

/home/git/doggohub-shell/doggohub-shell.log {
    daily
    missingok
    rotate 90
    compress
    notifempty
    copytruncate
}
